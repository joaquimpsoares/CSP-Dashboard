{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "cloudcake/laravel-approval": "^1.1",
        "doctrine/dbal": "^3.3",
        "guzzlehttp/guzzle": "^7.4",
        "itsgoingd/clockwork": "^5.1",
        "lab404/laravel-impersonate": "^1.7.7",
        "laravel-notification-channels/microsoft-teams": "^1.1",
        "laravel/breeze": "^2.0",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.9",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.0",
        "league/fractal": "^0.19.2",
        "livewire/livewire": "^3.0",
        "maatwebsite/excel": "^3.1",
        "mhmiton/laravel-modules-livewire": "^1.2",
        "michelf/php-markdown": "^1.9",
        "microsoft/microsoft-graph": "^1.85",
        "mkocansey/bladewind": "^1.0",
        "nwidart/laravel-modules": "^8.2",
        "php-http/guzzle7-adapter": "^1.0",
        "php-http/httplug-bundle": "^1.18",
        "predis/predis": "^1.1",
        "socialiteproviders/microsoft": "^4.0",
        "socialiteproviders/microsoft-graph": "^4.0",
        "spatie/laravel-permission": "^6.0",
        "spatie/laravel-searchable": "^1.11",
        "stripe/stripe-php": "^16.0",
        "tanmuhittin/laravel-google-translate": "^2.0",
        "webpatser/laravel-countries": "^1.5"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0",
        "laravel/sail": "^1.13",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "treeware/plant": false,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
