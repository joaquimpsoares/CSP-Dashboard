{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://gitlab.com/tagydes/microsoft-connection"
        },
        {
            "type": "vcs",
            "url": "https://gitlab.com/tagydes/kaspersky-connection.git"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "alajusticia/laravel-expirable": "^1.5",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "hwi/oauth-bundle": "^1.1",
        "jamesmills/laravel-timezone": "^1.9",
        "kordy/ticketit": "0.*",
        "lab404/laravel-impersonate": "^1.6",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.2",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.1",
        "league/fractal": "^0.19.2",
        "livewire/livewire": "^2.0",
        "maatwebsite/excel": "^3.1",
        "mhmiton/laravel-modules-livewire": "^1.2",
        "michelf/php-markdown": "^1.9",
        "nwidart/laravel-modules": "^8.2",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/httplug-bundle": "^1.18",
        "romanzipp/laravel-queue-monitor": "^2.0",
        "socialiteproviders/microsoft": "^4.0",
        "socialiteproviders/microsoft-graph": "^4.0",
        "soved/laravel-gdpr": "^1.5",
        "spatie/laravel-permission": "^3.11",
        "spatie/simple-excel": "^1.13",
        "tagydes/kaspersky-connection": "dev-master",
        "tagydes/microsoft-connection": "dev-master",
        "tanmuhittin/laravel-google-translate": "^2.0",
        "webpatser/laravel-countries": "^1.5",
        "yajra/laravel-datatables": "^1.5"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "laravel/sail": "^1.13",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "treeware/plant": false
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
